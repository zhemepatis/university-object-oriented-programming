<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="370.0" prefWidth="601.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ex4.controllers.StudentsList">
    <TableView fx:id="table" prefHeight="370.0" prefWidth="292.0" />

    <Label layoutX="301.0" layoutY="58.0" text="First name" />
    <TextField fx:id="firstNameField" layoutX="301.0" layoutY="76.0" prefHeight="26.0" prefWidth="128.0" />

    <Label layoutX="463.0" layoutY="58.0" text="Second name" />
    <TextField fx:id="secondNameField" layoutX="461.0" layoutY="76.0" prefHeight="26.0" prefWidth="128.0" />

    <Button layoutX="476.0" layoutY="109.0" mnemonicParsing="false" onAction="#addStudent" prefHeight="26.0" prefWidth="115.0" text="Add student" />
    <Button layoutX="476.0" layoutY="330.0" mnemonicParsing="false" onAction="#deleteStudent" prefHeight="26.0" prefWidth="115.0" text="Delete student" />
   <TextField fx:id="groupField" layoutX="301.0" layoutY="25.0" prefHeight="26.0" prefWidth="128.0" />
</AnchorPane>
